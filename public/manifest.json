{
  "manifest_version": 2,
  "name": "Chrome Extension Boilerplate",
  "description": "Chrome Boilerplate with CRA",
  "version": "1.0.0",
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "browser_action": {
    "default_title": "Chrome Extension Boilerplate"
  },
  "background": {
    "scripts": ["./jquery.js", "background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "all_frames": true,
      "js": ["./content.js", "./jquery.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["activeTab"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "web_accessible_resources": ["index.html", "/static/*"]
}
